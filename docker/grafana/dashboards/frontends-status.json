    },
    {
      "id": 4,
      "title": "Web App (Next.js)",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "collapsed": false
    },
    {
      "id": 5,
      "title": "Web App - Request Rate",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 6
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(web_app_http_requests_total[5m])) by (method, path) or vector(0)",
          "refId": "A",
          "legendFormat": "Web App Http Requests"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          },
          "noValue": "0",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "spanNulls": true,
            "gradientMode": "none",
            "showPoints": "never",
            "pointSize": 5
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
