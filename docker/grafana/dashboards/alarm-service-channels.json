    {
      "id": 6,
      "title": "Notifications Sent by Channel",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "(rate(email_sent_total[$__rate_interval])) or vector(0)",
          "refId": "A",
          "legendFormat": "Email/sec"
        },
        {
          "expr": "(rate(sms_sent_total[$__rate_interval])) or vector(0)",
          "refId": "B",
          "legendFormat": "SMS/sec"
        },
        {
          "expr": "(rate(voice_sent_total[$__rate_interval])) or vector(0)",
          "refId": "C",
          "legendFormat": "Voice/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          },
          "noValue": "0",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "spanNulls": true,
            "gradientMode": "none",
            "showPoints": "never",
            "pointSize": 5
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
