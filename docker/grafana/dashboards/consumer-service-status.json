    {
      "id": 4,
      "title": "Throughput by Queue",
      "type": "row",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "collapsed": false
    },
    {
      "id": 5,
      "title": "Messages Processed (Total)",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(consumer_service_messages_processed_total[5m])) by (job, queue) or vector(0)",
          "refId": "A",
          "legendFormat": "Consumer Service Messages Processed"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          },
          "noValue": "0",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "spanNulls": true,
            "gradientMode": "none",
            "showPoints": "never",
            "pointSize": 5
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
