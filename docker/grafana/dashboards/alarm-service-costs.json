    {
      "id": 2,
      "title": "Estimated Total Cost",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 1,
        "w": 6,
        "h": 5
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "((email_sent_total * 0.0001) or vector(0)) + ((sms_sent_total * 0.05) or vector(0)) + ((voice_sent_total * 0.02) or vector(0))",
          "refId": "A",
          "legendFormat": "Total",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 10,
                "color": "yellow"
              },
              {
                "value": 100,
                "color": "red"
              }
            ]
          },
          "color": {
            "mode": "thresholds"
          },
          "noValue": "0",
          "displayName": "Estimated Total Cost"
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value",
        "orientation": "horizontal",
        "justifyMode": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
