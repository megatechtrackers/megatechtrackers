        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "noValue": "0",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "lineWidth": 2,
            "spanNulls": true,
            "gradientMode": "none",
            "showPoints": "never",
