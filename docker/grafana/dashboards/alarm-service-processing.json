      "id": 1,
      "title": "Total Alarms Processed",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "alarms_processed_total or vector(0)",
          "refId": "A",
          "legendFormat": "Total",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "#3274D9"
          },
          "noValue": "0",
          "displayName": "Total Alarms Processed"
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value",
        "orientation": "horizontal",
        "justifyMode": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
