          "expr": "(rate(notification_delivery_time_ms_voice_bucket[$__rate_interval])) or vector(0)",
          "refId": "C",
          "legendFormat": "Voice P95 (sec)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "palette-classic"
          },
          "noValue": "0",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "spanNulls": true,
            "gradientMode": "none",
            "showPoints": "never",
            "pointSize": 5
