{
  "title": "Alarm Service - SLA Compliance",
  "uid": "sla-tracking",
  "tags": [
    "alarm",
    "sla",
    "compliance"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "SLA Compliance Rate by Channel",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "(notification_sla_compliance_email * 100) or vector(0)",
          "refId": "A",
          "legendFormat": "Email %"
        },
        {
          "expr": "(notification_sla_compliance_sms * 100) or vector(0)",
          "refId": "B",
          "legendFormat": "SMS %"
        },
        {
          "expr": "(notification_sla_compliance_voice * 100) or vector(0)",
          "refId": "C",
          "legendFormat": "Voice %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 90,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "green"
              }
            ]
          },
          "color": {
            "mode": "thresholds"
          },
          "noValue": "0",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineWidth": 2,
            "spanNulls": true,
            "gradientMode": "none",
            "showPoints": "never",
            "pointSize": 5
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 2,
      "title": "Delivery Time P95 by Channel",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "(rate(notification_delivery_time_ms_email_bucket[$__rate_interval])) or vector(0)",
          "refId": "A",
          "legendFormat": "Email P95 (sec)"
